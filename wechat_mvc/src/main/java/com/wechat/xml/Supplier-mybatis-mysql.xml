<?xml version="1.0" encoding="UTF-8" ?>   
    <!DOCTYPE mapper   
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
	<mapper namespace="com.wechat.model.Supplier"> 
	
	<select id="selectSupplierByUsername" parameterType="hashmap" resultType="Supplier">
		SELECT 
			*
		FROM cp_supplier 
		where code = #{username }
	</select>
</mapper>