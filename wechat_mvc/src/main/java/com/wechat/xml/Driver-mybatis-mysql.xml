<?xml version="1.0" encoding="UTF-8" ?>   
    <!DOCTYPE mapper   
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"   
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">  
	<mapper namespace="com.wechat.model.Driver"> 
	
	<select id="selectDriver" parameterType="hashmap" resultType="Driver">
		SELECT 
			*
		FROM CP_DRIVER 
		where tel = #{tel }
	</select>

</mapper>